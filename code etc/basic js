// creating array
const array = [x,y,z];


// function build
function loop(amout){
    for(let i = 0; i < amout; i++) {
    console.log(array[i]);
}

}

// function output
let la = loop(array.length);

// class build
class pet {
    constructor(name,type) {
        this.name = name;
        this.type = type;
    }
    walk(bool) {
        return bool;
    }
}
// class output
const dog = new pet("toto", "sibian")

console.log(dog.name,dog.type,dog.walk("yes"));